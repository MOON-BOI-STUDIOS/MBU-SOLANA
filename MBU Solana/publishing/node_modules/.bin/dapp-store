#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules/@solana-mobile/dapp-store-cli/bin/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules/@solana-mobile/dapp-store-cli/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules/@solana-mobile/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules/@solana-mobile/dapp-store-cli/bin/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules/@solana-mobile/dapp-store-cli/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules/@solana-mobile/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/@solana-mobile+dapp-store-cli@0.5.2/node_modules:/Volumes/ADATA_HD710/Moonboi/MBU-SOLANA/MBU Solana/publishing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --experimental-json-modules "$basedir/../@solana-mobile/dapp-store-cli/bin/dapp-store.js" "$@"
else
  exec node  --experimental-json-modules "$basedir/../@solana-mobile/dapp-store-cli/bin/dapp-store.js" "$@"
fi
